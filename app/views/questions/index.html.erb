<p id="notice"><%= notice %></p>

<h1>Questions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Subgenre</th>
      <th>Op1</th>
      <th>Op2</th>
      <th>Op3</th>
      <th>Op4</th>
      <!-- <th>Answer</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.title %></td>
        <td><%= question.subgenre.title %></td>
        <td><%= question.op1 %></td>
        <td><%= question.op2 %></td>
        <td><%= question.op3 %></td>
        <td><%= question.op4 %></td>
        <!-- <td><%= question.answer %></td> -->
        <td><%= link_to 'Show', question %></td>
        <!-- <td><%= link_to 'Edit', edit_question_path(question) %></td> -->
        <!-- <td><%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

