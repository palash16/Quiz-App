 <%= current_user.inspect %>
<p id="notice"><%= notice %></p>
<%= form_for(@question) do |f| %>
<p>
  <strong>Name:</strong>
  <%= label_tag(@question.title )%>
</p>


<p>
  <div class="radio-checkbox">
  <strong>A:</strong>
  <%= check_box_tag(:A) %>
  <%= label_tag(:A_false, @question.op1) %>
  </div>
</p>

<p>
  <strong>B:</strong>
  <%= check_box_tag(:B) %>
  <%= label_tag(:B_false, @question.op2) %>

</p>

<p>
  <strong>C:</strong>
  <%= check_box_tag(:C) %>
  <%= label_tag(:C_true, @question.op3) %>
</p>

<p>
  <strong>D:</strong>
  <%= check_box_tag(:D) %> 
  <%= label_tag(:D_false, @question.op4) %>
</p>

<%= submit_tag("Submit") %>

<%end%>


<%= link_to 'Back', '/questions/'%>